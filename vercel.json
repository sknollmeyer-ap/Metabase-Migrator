{
    "version": 2,
    "builds": [
        {
            "src": "ui/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "ui/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/ui/$1"
        }
    ],
    "env": {
        "METABASE_BASE_URL": "@metabase_base_url",
        "METABASE_API_KEY": "@metabase_api_key",
        "GEMINI_API_KEY": "@gemini_api_key",
        "OLD_DB_ID": "@old_db_id",
        "NEW_DB_ID": "@new_db_id",
        "GEMINI_MODEL": "@gemini_model"
    }
}
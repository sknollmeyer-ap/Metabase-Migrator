{
    "buildCommand": "cd ui && npm install && npm run build",
    "outputDirectory": "ui/dist",
    "devCommand": "cd ui && npm run dev",
    "installCommand": "npm install && cd ui && npm install",
    "framework": null,
    "functions": {
        "api/index.ts": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        }
    ]
}